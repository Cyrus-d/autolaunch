{
  "name": "autolaunch",
  "displayName": "AutoLaunch",
  "description": "Automatically run tasks in tasks.json or launch config in launch.json",
  "version": "1.0.0",
  "keywords": [
    "auto",
    "run",
    "launch",
    "tasks",
    "debug"
  ],
  "homepage": "https://github.com/philfontaine/autolaunch",
  "bugs": {
    "url": "https://github.com/philfontaine/autolaunch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philfontaine/autolaunch.git"
  },
  "publisher": "philfontaine",
  "license": "MIT",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other",
    "Debuggers"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "/.vscode/tasks.json",
        "url": "./custom-tasks-schema.json"
      },
      {
        "fileMatch": "/.vscode/launch.json",
        "url": "./custom-launch-schema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.3",
    "vscode": "^1.1.5",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}